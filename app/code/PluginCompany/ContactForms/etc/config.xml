<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
  <default>
    <plugincompany_contactforms>
      <form>
        <breadcrumbs>1</breadcrumbs>
        <frontendurl>1</frontendurl>
        <url_prefix>contact</url_prefix>
        <url_suffix>.html</url_suffix>
      </form>
      <customer_notification>
          <enable>1</enable>
      </customer_notification>
      <admin_notification>
        <enable>1</enable>
      </admin_notification>
    </plugincompany_contactforms>
    <dev>
      <css>
        <minify_exclude>
          .*backend.*PluginCompany_ContactForms.*
          .*frontend.*PluginCompany_ContactForms.*
        </minify_exclude>
      </css>
      <js>
        <minify_exclude>
          .*frontend.*PluginCompany_ContactForms.*lib.*
          .*frontend.*PluginCompany_ContactForms.*form.*
        </minify_exclude>
      </js>
    </dev>
  </default>
</config>